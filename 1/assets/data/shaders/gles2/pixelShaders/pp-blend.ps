#include "../commons/defines.sh"
#include "../commons/uniforms.sh"
#include "../commons/varyings.sh"


void main()
{

	lowp vec4 color1 = texture2D(BASEMAP,V_TexCoord0);
	lowp vec4 color2 = texture2D(BASEMAP1, V_TexCoord0);
	lowp vec4 color =  color2*(1.0-color1.a) + color1*color1.a;
	gl_FragColor = color;
}
